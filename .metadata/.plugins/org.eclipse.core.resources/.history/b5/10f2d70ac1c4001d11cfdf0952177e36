			public static void main(String[] args) {
		 
				   SafeVarargs leia = new Scanner(System.in);
				int numero, contador;
		 
		 
		 System.out.println("Mostre a tabuada do n√∫mero: ");
			numero = leia.nextInt();
			
			while(contador <= 10) {
			 System.out.println(numero + "x =" + contador + "="+ (numero*contador));
		 contador ++;
					
			}		
