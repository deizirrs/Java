package aula_03;

import java.util.Scanner;

public class Exer1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
     int numero;
     Scanner leia = new Scanner(System.in);
     
     for (int i = 0; i <10; i++) {
    	 
     System.out.printf("Digite o %dº número (maior ou igual a 0 e menor que 10): ", i + 1);
     numero = leia.nextInt(); 
	}
	}
}
